#include<iostream>
#include<string>

using namespace std;

class A
{
    public:
        A(int _x, char _y, string _str)
        {
            x =_x;
            y = _y;
            str = _str;
        }
        ~A() { }

    private:
        char y;
        int x;
        string str;
};

int main()
{
    A a(2, 'x', "abc");

    return 0;
}
